{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "a9229864-7ca0-4374-b5dd-d02072c72948"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "localcorner": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/localcorner",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/assets/bootstrap.min.css",
              "src/assets/css/assets/font-awesome.min.css",
              "src/assets/css/assets/animate.css",
              "src/assets/css/assets/owl.carousel.min.css",
              "src/assets/css/assets/normalize.css",
              "src/assets/css/style.css",
              "src/assets/css/assets/responsive.css"
            ],
            "scripts": [
              "src/assets/js/assets/vendor/jquery-1.12.4.min.js",
              "src/assets/js/assets/popper.min.js",
              "src/assets/js/assets/bootstrap.min.js",
              "src/assets/js/assets/owl.carousel.min.js",
              "src/assets/js/assets/wow.min.js",
              "src/assets/js/assets/price-filter.js",
              "src/assets/js/assets/jquery.meanmenu.min.js",
              "src/assets/js/plugins.js",
              "src/assets/js/custom.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "20mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "localcorner:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "localcorner:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "localcorner:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "localcorner:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "localcorner:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "localcorner"
}
